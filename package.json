    "@types/crypto-js": "^4.1.1",
    "@types/elliptic": "^6.4.14",
    "@types/lodash": "^4.14.181",
    "@types/node": "^17.0.23",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "prettier": "^2.6.2",
    "ts-node": "^10.7.0",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3"
